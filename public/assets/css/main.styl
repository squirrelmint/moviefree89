	&:hover
	&.plyr__tab-focus
		background-color rgba($red, .1)
		color $red

.plyr--video .plyr__progress__buffer
	box-shadow(none)

.plyr__progress__buffer
	border-radius(0)

.plyr--full-ui input[type="range"]
